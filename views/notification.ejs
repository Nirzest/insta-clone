            justify-content: center;
            flex-direction: column;
            color: white;
        }
        .follo{
            display: flex;
            align-items: center;
            justify-content: center;
            height: 100%;
            width: 35%;
            /* background-color: white; */
        }
        button{
            width: 97%;
            height: 40%;
            background-color: #0395F6;
            color: white;
            font-weight: 500;
            font-size: 16px;
            border-radius: 10px;
        }
         .month{
            padding-top: 1vh;
        }
        .image{
            width: 70px;
            height: 70px;
            /* background-color: red; */
            display: flex;
            align-items: center;
            justify-content: flex-end;
        }
        .image img{
            width: 70px;
            height: 100%;
            object-fit: cover;
            object-position: top;
            /* border-radius: 50px; */
        }
        .name1{
