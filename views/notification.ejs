            font-weight: 600;
            color: white;
            text-transform: capitalize;
        }
        .request{
            display: flex;
            align-items: center;
            padding: 0 5vw;
            height: 10%;
            width: 100%;
            /* background-color: red; */
        }
        .ima{
            position: relative;
            width: 65px;
            height: 65px;
            /* background-color: blue; */
            border-radius: 50%;
        }
        .ima img{
            position: absolute;
            width: 100%;
            height: 100%;
            object-fit: cover;
            object-position: top;
            border-radius: 50px;
            z-index: 10;
        }
        .ima img:nth-child(2){
            position: absolute;
            top: -20%;
            right: 20%;
            z-index: 2;
        }
        .requ{
            padding-left: 2vw;
            height: 100%;
            width: 50%;
            /* background-color: red; */
            display: flex;
            justify-content: center;
