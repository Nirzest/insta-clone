      }
    .footer{
    padding: 0 2vw;
    position: fixed;
    width: 100%;
    height: 8vh;
    background-color: black;
    top: 93%;
    z-index: 10;
    display: flex;
    align-items: center;
    justify-content: space-between;
    }
    .footer a{
    text-decoration: none;
}
    .footer i{
    font-size: 35px;
    color: white;
    }

    .footer img{
    width: 40px;
    height: 40px;
    /* background-color: crimson; */
    border-radius: 50%;
    object-fit: cover;
    object-position: top;
    }
    .main{
        width: 100%;
        height: 93.1%;
        background-color: black;
    }
    .uppe{
        width: 100%;
        height: 7%;
        /* background-color: red; */
